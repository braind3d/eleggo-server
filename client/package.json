{
  "name": "eleggo",
  "author": "FAnton",
  "version": "0.1.0",
  "private": false,
  "homepage": "./",
  "main": "./public/electron.js",
  "description": "Programmable brain computer interface software",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "concurrently": "^6.2.1",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "recharts": "^2.0.8",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "concurrently 'BROWSER=none npm start' 'wait-on http://localhost:3000 && electron .'",
    "electron-build": "npm run build ; npx electron-builder -mwl -c.extraMetadata.main=./build/electron.js"
  },
  "build": {
    "appId": "com.fanton.eleggo",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "npmRebuild": false,
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}